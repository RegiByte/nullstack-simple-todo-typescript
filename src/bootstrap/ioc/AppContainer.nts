import {Container} from "inversify";
import {NullstackIocContainer} from "../internals/NullstackIocContainer.nts";

export class AppContainer extends Container implements NullstackIocContainer{
	constructor(props: object = {}) {
		super({
			skipBaseClassChecks: false,
			autoBindInjectable: false,
			...props,
		});
	}

	getName(): string {
		return "App";
	}
}
